{
  "name": "are-we-pretty-yet-client",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "yarn build:dev && yarn serve:dev",
    "start:prod": "yarn build:prod && yarn serve:prod",
    "build:dev": "webpack --config webpack/webpack.config-dev.babel.js",
    "build:prod": "webpack --config webpack/webpack.config-prod.babel.js",
    "serve:web:dev": "webpack-dev-server --config webpack/webpack.config-dev.babel.js",
    "serve:backend:dev": "nodemon --exec babel-node src/server/index.js",
    "serve:backend:prod": "babel-node src/server/index.js",
    "serve:dev": "yarn serve:web:dev & yarn serve:backend:dev",
    "serve:prod": "yarn serve:backend:prod",
    "npm:start:prod": "npm run npm:build:prod && npm run npm:serve:prod",
    "npm:build:prod": "webpack --config webpack/webpack.config-prod.babel.js",
    "npm:serve:backend:prod": "babel-node src/server/index.js",
    "npm:serve:prod": "npm run serve:backend:prod"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "copy-webpack-plugin": "^5.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.18.10",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.4",
    "apicache": "^1.4.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "globby": "^9.1.0",
    "hastebin-gen": "^1.3.1",
    "immer": "^2.1.3",
    "ky": "^0.9.0",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "recharts": "^1.5.0",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.5",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  }
}
